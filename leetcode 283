class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        fast = 0
        slow = 0
        size = len(nums)
        while fast < size:
            if nums[fast] != 0:
                nums[slow] = nums[fast]
                slow +=1
            fast +=1
        zero =slow
        while zero < size:
            nums[zero] = 0
            zero += 1
