class Solution:
    def mySqrt(self, x: int) -> int:
        if x == 1:
            return 1
        left, right = 0, x
        while left <= right:
            middle = (left + right) // 2
            if middle * middle > x:
                right = middle - 1
            elif (middle + 1) * (middle + 1) <= x:
                left = middle + 1
            else:
                return middle
