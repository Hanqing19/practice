class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        fast = 0
        slow = 0
        size = len(nums)
        while fast < size - 1:
            if nums[fast] != nums[fast + 1]:
                nums[slow + 1] = nums[fast + 1]
                slow += 1
            fast += 1
        return slow + 1
